{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch topstartups.js",
            "program": "${workspaceFolder}/topstartups.js",
            "runtimeExecutable": "node",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": ["<node_internals>/**"],
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}
